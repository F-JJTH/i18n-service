    "i18n-web": {
      "projectType": "application",
      "root": "apps/i18n-web",
      "sourceRoot": "apps/i18n-web/src",
      "prefix": "kizeo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/i18n-web",
            "index": "apps/i18n-web/src/index.html",
            "main": "apps/i18n-web/src/main.ts",
            "polyfills": "apps/i18n-web/src/polyfills.ts",
            "tsConfig": "apps/i18n-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/i18n-web/src/favicon.ico",
              "apps/i18n-web/src/assets"
            ],
            "styles": ["apps/i18n-web/src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@aws-crypto/crc32",
              "@aws-crypto/sha256-browser",
              "@aws-crypto/sha256-js",
              "buffer",
              "camelcase-keys",
              "crypto-js",
              "fast-xml-parser",
              "isomorphic-unfetch",
              "lodash",
              "qrcode",
              "ulid",
              "uuid"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/i18n-web/src/environments/environment.ts",
                  "with": "apps/i18n-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "i18n-web:build:production"
            },
            "development": {
              "browserTarget": "i18n-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "i18n-web:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/i18n-web/src/**/*.ts",
              "apps/i18n-web/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/i18n-web"],
          "options": {
            "jestConfig": "apps/i18n-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "i18n-web-e2e": {
      "root": "apps/i18n-web-e2e",
      "sourceRoot": "apps/i18n-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/i18n-web-e2e/cypress.json",
            "devServerTarget": "i18n-web:serve:development"
          },
          "configurations": {
            "production": {
